{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Api Library",
  "extends": "./base.json",
  "compilerOptions": {
    "lib": ["DOM", "DOM.Iterable", "ES2019"],
    "isolatedModules": true,
    "module": "ESNext",
    "resolveJsonModule": true,
    "target": "ES2019",
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["./src/*"]
    }
  }
}
